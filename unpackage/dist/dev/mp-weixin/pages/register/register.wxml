ent']]]]]}}" value="{{password}}" bindinput="__e"/></view><view class="input"><input class="uni-input" valueww="{{repeat}}" type="password" placeholder="{{i18n.pleaseAgainPass}}" data-event-opts="{{[['input',[['__set_model',['','repeat','$event',[]]],['repeatsearch',['$event']]]]]}}" value="{{repeat}}" bindinput="__e"/></view><view class="agree"><block wx:if="{{show}}"><radio value="{{ra}}" checked="true" data-event-opts="{{[['tap',[['change',['$event']]]]]}}" bindtap="__e"></radio></block><block wx:else><radio value="{{ra}}" data-event-opts="{{[['tap',[['change',['$event']]]]]}}" bindtap="__e"></radio></block><view><text>{{i18n.yueDuTongYi}}</text><text data-event-opts="{{[['tap',[['yinsi',['$event']]]]]}}" class="xieyi" bindtap="__e">{{"《"+i18n.yinSiZhengCe+"》"}}</text></view><view><text data-event-opts="{{[['tap',[['xieyi',['$event']]]]]}}" class="xieyi" bindtap="__e">{{"《"+i18n.shiYongXieYi+"》"}}</text></view></view><view data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="submit" bindtap="__e">{{i18n.register}}</view><view data-event-opts="{{[['tap',[['zhuce',['$event']]]]]}}" class="redister" bindtap="__e">{{i18n.goLoginReg}}</view></view>